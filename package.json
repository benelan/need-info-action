{
  "name": "need-info-action",
  "version": "1.3.1",
  "private": true,
  "description": "A GitHub Action that requests more info when required content is not included in an issue",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "package": "ncc build --source-map --license licenses.txt",
    "release": "standard-version",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint:fix && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benelan/need-info-action.git"
  },
  "keywords": [
    "actions",
    "need info",
    "automation"
  ],
  "author": "Ben Elan",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.7.0",
    "@actions/github": "^5.0.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/ncc": "^0.33.4",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^26.1.5",
    "jest": "^27.1.1",
    "prettier": "2.6.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}
